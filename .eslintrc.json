{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "globals": {
    "Kanban": "readonly"
  },
  "rules": {
    "no-unused-vars": ["warn", { "args": "none", "varsIgnorePattern": "^K$" }],
    "no-console": "off"
  }
}
